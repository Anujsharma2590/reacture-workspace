{
  "name": "@reacture/core",
  "version": "2.2.0",
  "description": "A core set of React UI components",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "ui",
    "components",
    "library",
    "component",
    "design",
    "framework",
    "frontend",
    "react-component"
  ],
  "sideEffects": false,
  "author": "Anuj Sharma <aanuj.sharma2590@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Anujsharma2590/reacture-workspace.git",
    "directory": "packages/core"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build:cjs": "tsc --project tsconfig.json --module CommonJS --outDir dist/cjs",
    "build:esm": "tsc --project tsconfig.json --module ESNext --outDir dist/esm",
    "build": "yarn build:cjs && yarn build:esm",
    "dev": "tsc --watch --project tsconfig.json",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "@reacture/theme": "1.0.0"
  },
  "devDependencies": {
    "@types/react": ">=16.8.0 <19.0.0",
    "@types/react-dom": ">=16.8.0 <19.0.0",
    "typescript": "^5.6.3",
    "eslint": "^8.0.0",
    "jest": "^28.0.0",
    "rimraf": "^6.0.0"
  }
}
